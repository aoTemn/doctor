        "ANDROID_PLAY_SERVICES_TAGMANAGER_VERSION": "18.0.2",
        "ANDROID_PLAY_SERVICES_AUTH_VERSION": "20.5.0",
        "ANDROID_FIREBASE_ANALYTICS_VERSION": "21.2.2",
